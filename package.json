{
  "name": "observe-ts",
  "version": "0.3.0",
  "description": "Deps, errors, logs, traces all tracked for all code",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/observe-ts/observe-ts.git"
  },
  "bugs": {
    "url": "https://github.com/observe-ts/observe-ts/issues"
  },
  "homepage": "https://github.com/observe-ts/observe-ts",
  "keywords": [
    "effect",
    "effect-ts",
    "functional-programming",
    "typescript",
    "library"
  ],
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "dev": "tsup --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@effect/opentelemetry": "^0.59.1",
    "@opentelemetry/sdk-metrics": "^2.2.0",
    "@opentelemetry/sdk-trace-base": "^2.2.0",
    "@opentelemetry/sdk-trace-node": "^2.2.0",
    "@opentelemetry/sdk-trace-web": "^2.2.0",
    "effect": "^3.19.5"
  },
  "devDependencies": {
    "@effect/opentelemetry": "^0.59.1",
    "@opentelemetry/sdk-logs": "^0.203.0",
    "@opentelemetry/sdk-metrics": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/sdk-trace-base": "^2.2.0",
    "@opentelemetry/sdk-trace-node": "^2.2.0",
    "@opentelemetry/sdk-trace-web": "^2.2.0",
    "@types/node": "^20.11.30",
    "effect": "^3.19.5",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.4.5",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=18.17"
  },
  "publishConfig": {
    "access": "public"
  }
}
